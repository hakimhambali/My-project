:root {
    --background-img: url('project://database/Assets/Sprites/bg-11.png');
    --background-img2: url('project://database/Assets/Sprites/hang_tuah_avatar.png');
    --background-img3: url('project://database/Assets/Sprites/hang-tuah2.png');
    --background-img4: url('project://database/Assets/Sprites/hang-tuah-avatar-round.png');
    --background-img5: url('project://database/Assets/Sprites/mic-icon.png');
    --background-img7: url('project://database/Assets/Sprites/mic-icon2.png');
    --background-img6: url('project://database/Assets/Sprites/send-icon.png');
    --background-img8: url('project://database/Assets/Sprites/send-icon2.png');
    --background-img9: url('project://database/Assets/Sprites/chat-icon.png');
    --background-img10: url('project://database/Assets/Sprites/skill.png');
    --background-img11: url('project://database/Assets/Sprites/skill2.png');
    --background-img12: url('project://database/Assets/Layer%20Lab/GUI%20Pro-FantasyRPG/ResourcesData/Sprites/Component/Button/btn_rectangle_01_n_yellow.png');
    --background-img13: url('project://database/Assets/Sprites/time.png');
